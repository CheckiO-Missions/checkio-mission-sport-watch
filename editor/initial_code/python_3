def verificar_sugestao(frequencia_cardiaca_repouso, frequencia_cardiaca_atual, capacidade_oxigenacao_atual):
    # escreva aqui seu código
    return None


print("Example:")
print(verificar_sugestao(60, 190, 98))

# As funções assert são utilizadas para verificar se o resultado da função chamada é o esperado
# Caso em que a frequência cardíaca atual é maior que três vezes a frequência cardíaca em repouso
assert verificar_sugestao(60, 190, 98) == 'diminuir'

# Caso em que a capacidade de oxigenação atual é menor que 95
assert verificar_sugestao(70, 140, 92) == 'diminuir'

# Caso em que a frequência cardíaca atual é menor que duas vezes a frequência cardíaca em repouso
# e a capacidade de oxigenação atual é maior que 97
assert verificar_sugestao(70, 130, 98) == 'aumentar'

# Caso em que nenhuma das condições é satisfeita
assert verificar_sugestao(65, 155, 96) == 'manter'
    
